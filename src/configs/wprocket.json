  "buttonsecondaryboxshadow": "@buttonboxshadow",
  "buttonsecondarycolor": "@whitecolor",
  "buttonsecondaryfontsize": "@buttonfontsize",
  "buttonsecondarymargin": "0",
  "buttonsecondarypadding": "2px 14px",
  "buttonsecondarytextshadow": "none",
  "buttonsecondarytexttransform": "uppercase",

  "buttonsecondaryhoverbackgroundcolor": "darken(@secondarycolor, 10%)",
  "buttonsecondaryhoverbackgroundimage": "none",
  "buttonsecondaryhoverborder": "none",
  "buttonsecondaryhoverborderradius": "@defaultborderradius",
  "buttonsecondaryhoverboxshadow": "@buttonhoverboxshadow",
  "buttonsecondaryhovercolor": "@whitecolor",
  "buttonsecondaryhoverfontsize": "@buttonsecondaryfontsize",
  "buttonsecondaryhovermargin": "0",
  "buttonsecondaryhoverpadding": "2px 14px",
  "buttonsecondaryhovertextshadow": "none",
  "buttonsecondaryhovertexttransform": "uppercase",

  "buttonsecondarytransition": "@defaulttransition",



  "buttonheroboxshadow": "@buttonprimaryboxshadow",
  "buttonherofontsize": "14px",
  "buttonheropadding": "12px 36px",

  "buttonherohoverboxshadow": "@buttonprimaryhoverboxshadow",
  "buttonherohoverfontsize": "@buttonherofontsize",
  "buttonherohoverpadding": "@buttonheropadding",



