  "sidebarpadding": "0",
  "sidebartexttransform": "uppercase",
  "sidebarwidth": "@defaultsidebarwidth",

  "dropdownbackgroundcolor": "@adminbarhoverbackgroundcolor",
  "dropdownboxshadow": "@buttonboxshadow",
  "dropdowncolor": "@adminbarcolor",
  "dropdownhoverbackgroundcolor": "@adminbarhoverbackgroundcolor",
  "dropdownhovercolor": "@adminbarhovercolor",
  "dropdownfontsize": "@adminbarfontsize",
  "dropdownmargin": "0",
  "dropdownpadding": "8px 0",
  "dropdownwidth": "220px",



  "boxcontainerbackgroundcolor": "@bodybackgroundcolor",
  "boxcontainerborder": "none",
  "boxcontainerborderradius": "0",
  "boxcontainerboxshadow": "none",
  "boxcontainercolor": "#676767",
  "boxcontainerpadding": "0",

  "boxheadercolor": "@primarycolor",
  "boxheaderfontsize": "1rem",
