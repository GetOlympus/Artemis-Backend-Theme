  "inputfocusborderradius": "@inputborderradius",
  "inputfocusboxshadow": "none",
  "inputfocuscolor": "@inputcolor",
  "inputfocusfontsize": "@inputfontsize",
  "inputfocuspadding": "@inputpadding",

  "inputtransition": "@defaulttransition",



