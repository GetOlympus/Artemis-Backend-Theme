{
  "name": "WP Rocket",
  "file": "wprocket",



  "primarycolor": "#f56640",
  "secondarycolor": "#2d1656",
  "thirdcolor": "#0e1a22",

  "whitecolor": "#ffffff",
  "blackcolor": "#0e1b23",
  "infocolor": "#2185d0",
  "warningcolor": "#f2711c",
  "errorcolor": "#db2828",
  "successcolor": "#1ebc30",

  "defaultbordercolor": "@secondarycolor",
  "defaultborderradius": "3px",
  "defaulttransition": "all .250s ease",
  "defaultsidebarwidth": "225px",



  "bodybackgroundcolor": "#f6f6f6",
  "bodypadding": "20px",



  "linkcolor": "#1b9aaa",
  "linktextdecoration": "underline",
  "linktexttransform": "none",

  "linkhovercolor": "#026371",
  "linkhovertextdecoration": "underline",
  "linkhovertexttransform": "none",

  "linktransition": "@defaulttransition",



  "inputbackgroundcolor": "@whitecolor",
  "inputborder": "2px solid #c2cad4",
  "inputborderradius": "@defaultborderradius",
  "inputboxshadow": "none",
  "inputcolor": "@blackcolor",
  "inputfontsize": "100%",
  "inputpadding": "6px 8px",

  "inputfocusbackgroundcolor": "@inputbackgroundcolor",
  "inputfocusborder": "2px solid #444",
  "inputfocusborderradius": "@inputborderradius",
  "inputfocusboxshadow": "none",
  "inputfocuscolor": "@inputcolor",
  "inputfocusfontsize": "@inputfontsize",
  "inputfocuspadding": "@inputpadding",

  "inputtransition": "@defaulttransition",



  "adminbarbackgroundcolor": "@thirdcolor",
  "adminbarborderbottom": "none",
  "adminbarcolor": "@whitecolor",
  "adminbarhoverbackgroundcolor": "#121f27",
  "adminbarhovercolor": "@whitecolor",
