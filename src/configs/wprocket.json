  "tablecolor": "@blackcolor",
  "tabletitlecolor": "@linkcolor",



  "loginbackgroundcolor": "@bodybackgroundcolor",
  "loginbackgroundimage": "url(../img/wprocket-login.svg), linear-gradient(to bottom, #0e1b23, #0e1b23 300px, #f6f6f6 300px)",
  "loginbackgroundsize": "auto",
  "logincolor": "#0e1b23",
  "loginfontsize": "1.9rem",
  "loginlineheight": "1.5em",

  "logincontainerbackgroundcolor": "@whitecolor",
  "logincontainerborder": "1px solid #e5e5e5",
  "logincontainerborderradius": "0",
  "logincontainerboxshadow": "none",
  "logincontainermargin": "30px auto",
  "logincontainerpadding": "30px 42px",
